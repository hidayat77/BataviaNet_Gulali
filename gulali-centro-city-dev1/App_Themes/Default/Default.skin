<asp:textbox runat="server" skinid="tgl" width="70" cssclass="center" />

<asp:button runat="server" skinid="login" text="Login" width="75" accesskey="l" />
<asp:button runat="server" skinid="save" text="Save" width="75" accesskey="s" />
<asp:button runat="server" skinid="del" text="Delete" width="75" accesskey="d" />
<asp:button runat="server" skinid="display" text="Display" width="75" accesskey="d" />
<asp:button runat="server" skinid="next" text="Next" width="75" accesskey="n" />
<asp:button runat="server" skinid="finish" text="Finish" width="75" accesskey="f" />
<asp:button runat="server" skinid="upload" text="Upload" width="75" accesskey="u" />

<asp:requiredfieldvalidator runat="server" errormessage="Empty" />
<asp:regularexpressionvalidator runat="server" skinid="pk" errormessage="Aplha Numeric" />
<asp:regularexpressionvalidator runat="server" skinid="email" errormessage="Email" />
<asp:comparevalidator runat="server" skinid="pass" errormessage="Failed Confirm" />
<asp:comparevalidator runat="server" skinid="tgl" errormessage="Date Time" />
<asp:comparevalidator runat="server" skinid="int" errormessage="Number" />

<asp:table runat="server" cssclass="list" />
<asp:table runat="server" skinid="form" cssclass="data" />
<asp:table runat="server" skinid="grid" cssclass="list" borderwidth="1" gridlines="both" />

<asp:gridview runat="server" cssclass="list"
	gridlines="None" autogeneratecolumns="False"
	emptydatarowstyle-cssclass="kosong" />
<asp:gridview runat="server" skinid="pager" cssclass="list" 
	gridlines="None" autogeneratecolumns="False"
	emptydatarowstyle-cssclass="kosong"
	emptydatatext="Data for the selected criteria is unavailable."
	pagersettings-firstpagetext="First"
	pagersettings-lastpagetext="Last"
	pagerstyle-cssclass="pager"
	allowpaging="true"
	pagersettings-position="TopAndBottom"
	pagesize="20" />
	
<asp:calendar runat="server" weekenddaystyle-forecolor="red" todaydaystyle-backcolor="lightblue"
	dayheaderstyle-font-bold="True" othermonthdaystyle-forecolor="Silver" cellpadding="3"  />